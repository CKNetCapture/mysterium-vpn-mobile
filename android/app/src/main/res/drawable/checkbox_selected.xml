<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="20dp"
                android:height="20dp" />
            <corners android:radius="5dp" />
            <stroke
                android:width="1dp"
                android:color="#E262AA" />
            <solid android:color="#D61F85" />
        </shape>
    </item>
    <item
        android:drawable="@drawable/icon_checkbox_mark"
        android:gravity="center" />
</layer-list>
